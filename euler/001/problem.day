import "euler/helpers.day"


fun problem001 () int do
	(welcome 1)
	
	let solution: int <- (_problem001 1000)
	(assert (check_solution solution 233168))
	solution
end

fun _problem001 (limit: int) int do
	let n: int <- 3
	let m: int <- 5
	let nm: int <- (mul n m)
	
	let threes: int <- (partial_sum limit n)
	let fives: int <- (partial_sum limit m)
	let combined: int <- (partial_sum limit nm)
	
	(sub 
		(add
			threes
			fives
		)
		combined)
end
