{
  "triggerTaskOnSave.tasks": {
    "cmake build [daybreak]": [
      "src/include/**/*.h",
      "src/main.c",
    ],
    "cmake build [daybreak_test]": [
      "src/include/**/*.h",
      "src/main.c",
      "tests/*.h",
      "tests/test_main.c",
    ],
    "cmake configure": [
      "CMakeLists.txt",
      "**/CMakeLists.txt",
    ],
  },
  "files.associations": {
    "*.script": "lua",
    "*.py": "python",
    "*.dron": "jsonc",
    "*.fs": "fsharp",
    "*.h": "c",
    "*.c": "c",
    "typeinfo": "c",
    "type_traits": "c",
    "limits": "c"
  },
  "cSpell.words": [
    "ccstring"
  ],
}